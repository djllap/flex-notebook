<!DOCTYPE html>
<html>
  <head>
    <title>Flexnotebook</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  

  <body>
    <div>
      <%= render "layouts/header" %>
    </div>

    <div class="container">
      <%= yield %>        
    </div>
  </body>

  <div class="container-fullwidth navbar navbar-default navbar-fixed-bottom">
    <div class="text-center nav-justified">
      <%= render "layouts/footer" %>
    </div>
  </div>
</html>
